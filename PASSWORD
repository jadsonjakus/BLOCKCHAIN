# Importar a biblioteca random
import random

# Definir uma lista de caracteres possíveis para a senha
characters = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!@#$%^&*()"

# Definir o comprimento da senha (você pode mudar esse valor)
length = 10

# Criar uma lista vazia para armazenar a senha
password = []

# Para cada posição na senha, escolher um caractere aleatório da lista de caracteres
for i in range(length):
    password.append(random.choice(characters))

# Converter a lista de caracteres em uma string
password = "".join(password)

# Imprimir a senha gerada
print(password)
